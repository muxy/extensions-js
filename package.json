{
  "name": "@muxy/extensions-js",
  "author": "Muxy, Inc.",
  "version": "2.3.0",
  "license": "ISC",
  "repository": "https://github.com/muxy/extensions-js",
  "description": "Provides easy access to Muxy's powerful backend architecture for Twitch extensions.",
  "main": "dist/medkit.umd.js",
  "module": "dist/medkit.esm.js",
  "typings": "dist/types/src/index.d.ts",
  "scripts": {
    "lint": "npx eslint 'src/**/*.ts' '__tests__/**/*.ts'",
    "dev": "rollup -c rollup.config.ts -w",
    "build": "tsc --module commonjs --skipLibCheck && rollup -c rollup.config.ts",
    "test": "jest __tests__/*.ts",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "{src,__tests__}/**/*.ts": [
      "prettier --config ./.prettierrc.js --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "verbose": true,
    "browser": true
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/pusher-js": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "colors": "^1.3.2",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.2",
    "cross-env": "^5.1.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^1.1.2",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.21.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.2",
    "semantic-release": "^15.13.24",
    "ts-jest": "^24.1.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.15.0",
    "typedoc-plugin-external-module-name": "^1.1.3",
    "typedoc-plugin-sourcefile-url": "^1.0.4",
    "typescript": "^3.6.3",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "eslint-config-prettier": "^6.3.0",
    "pako": "^1.0.10",
    "pusher-js": "^4.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
